			{ 
				val 0 
			}
			right 
			{ 
				val 320 
			}
			top 
			{ 
				val 40 
			}
			bottom 
			{ 
				val 480 
			}
		}
		
		element 
		{
			name "PanelContainer"

			top 
			{ 
				val 233
			}
			right 
			{ 
				val 320
			}
			left 
			{ 
				val 0
			}
			bottom
			{ 
				val 480
			}
			alignV "bottom"
		}
	}
	
	CIwUIFocusHandlerTab 
	{
		name "Focus1"
	}
