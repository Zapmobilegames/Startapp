CIwUIPropertySet 
{
	name "<balloon_style>"

	font "times"
	textColour { 45 45 45 255 }
	alignH "centre"
	alignV "middle"
	displayLength 300
	balloonSize  { 70 70 }
    background
    {
        drawableType "border"
        texture "textField_background"
	    border  { 12 12 }
    }
}

CIwUIPropertySet 
{
	name "<softkeyboard_all_in_one>"

    font        "times"
	textColour  { 45 45 45 255 }
	alignH 		"centre"
	alignV 		"middle"

	buttonUp
    {
        drawableType "border"
		texture "button_up"
        border  { 12 12 }
    }

    buttonDown
    {
		drawableType "border"
		texture "button_down"
        border  { 12 12 }
    }
}

CIwUIPropertySet 
{
	name "<softkeyboard_lower_key_label>"

	sizeToContent   true
	font            "times"
	textColour      { 45 45 45 255 }
	alignH 		    "centre"
	alignV 		    "middle"
}

CIwUIPropertySet 
{
	name "<softkeyboard_keys_style>"

	wrapLeft "wrap"
	wrapRight "wrap"
	wrapUp "none"
	wrapDown "stop"
}

CIwUISoftKeyboard 
{
	name "soft_keyboard"

	sizeToContent true
	cursorDelay 1500
	cursorFlashPeriod 1200

    CIwUIElement 
	{
		name "PanelContainer"
