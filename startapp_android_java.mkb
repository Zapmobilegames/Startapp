#!/usr/bin/env mkb
# Builder mkb file for the java portion of the startapp extension on android
platform JAVA

files
{
    (source/android)
    startapp.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "$MARMALADE_ROOT/extensions/startapp/StartAppCommon-1.0.1.jar"
librarypath "$MARMALADE_ROOT/extensions/startapp/StartAppInApp-2.0.3.jar"
librarypath "$MARMALADE_ROOT/extensions/startapp/StartAppSearchBox-1.0.2.jar"

option output-name=lib/android/startapp.jar
